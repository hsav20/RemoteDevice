<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ltd.advskin.view.KcHeaderView
        android:id="@+id/khvNoiseTest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <!--前置左-->
    <ltd.advskin.view.KcBtnText
        android:id="@+id/kbtNoiseTestA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:gravity="center"
        app:textSize="14sp"
        app:layout_constraintTop_toTopOf="@+id/kbtNoiseTestC"
        app:layout_constraintLeft_toLeftOf="@+id/havNoiseTestA"
        app:layout_constraintRight_toRightOf="@+id/havNoiseTestA"/>
    <ltd.kcdevice.view.VerticalAdjView
        android:id="@+id/havNoiseTestA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        app:layout_constraintTop_toBottomOf="@+id/kbtNoiseTestA"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <!--前置右-->
    <ltd.advskin.view.KcBtnText
        android:id="@+id/kbtNoiseTestB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:gravity="center"
        app:textSize="14sp"
        app:layout_constraintTop_toTopOf="@+id/kbtNoiseTestC"
        app:layout_constraintLeft_toLeftOf="@+id/havNoiseTestB"
        app:layout_constraintRight_toRightOf="@+id/havNoiseTestB"/>
    <ltd.kcdevice.view.VerticalAdjView
        android:id="@+id/havNoiseTestB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="20dp"
        app:layout_constraintTop_toBottomOf="@+id/kbtNoiseTestB"
        app:layout_constraintRight_toRightOf="parent"/>

    <!--中置-->
    <ltd.advskin.view.KcBtnText
        android:id="@+id/kbtNoiseTestC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        app:textSize="14sp"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@+id/khvNoiseTest"
        app:layout_constraintLeft_toLeftOf="@+id/havNoiseTestC"
        app:layout_constraintRight_toRightOf="@+id/havNoiseTestC"/>
    <ltd.kcdevice.view.VerticalAdjView
        android:id="@+id/havNoiseTestC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/kbtNoiseTestC"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

    <!--超低音-->
    <ltd.advskin.view.KcBtnText
        android:id="@+id/kbtNoiseTestH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:gravity="center"
        app:textSize="14sp"
        app:layout_constraintTop_toTopOf="@+id/kbtNoiseTestC"
        app:layout_constraintLeft_toLeftOf="@+id/havNoiseTestH"
        app:layout_constraintRight_toRightOf="@+id/havNoiseTestH"/>
    <ltd.kcdevice.view.VerticalAdjView
        android:id="@+id/havNoiseTestH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/kbtNoiseTestH"
        app:layout_constraintLeft_toRightOf="@+id/havNoiseTestC"
        app:layout_constraintRight_toLeftOf="@+id/havNoiseTestB"/>

    <!--环绕左-->
    <ltd.advskin.view.KcBtnText
        android:id="@+id/kbtNoiseTestD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center"
        app:textSize="14sp"
        app:layout_constraintTop_toBottomOf="@+id/havNoiseTestA"
        app:layout_constraintLeft_toLeftOf="@+id/havNoiseTestD"
        app:layout_constraintRight_toRightOf="@+id/havNoiseTestD"/>
    <ltd.kcdevice.view.VerticalAdjView
        android:id="@+id/havNoiseTestD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        app:layout_constraintTop_toBottomOf="@+id/kbtNoiseTestD"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <!--环绕右-->
    <ltd.advskin.view.KcBtnText
        android:id="@+id/kbtNoiseTestE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center"
        app:textSize="14sp"
        app:layout_constraintTop_toBottomOf="@+id/havNoiseTestA"
        app:layout_constraintLeft_toLeftOf="@+id/havNoiseTestE"
        app:layout_constraintRight_toRightOf="@+id/havNoiseTestE"/>
    <ltd.kcdevice.view.VerticalAdjView
        android:id="@+id/havNoiseTestE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="20dp"
        app:layout_constraintTop_toBottomOf="@+id/kbtNoiseTestE"
        app:layout_constraintRight_toRightOf="parent"/>

    <!--后置左-->
    <ltd.advskin.view.KcBtnText
        android:id="@+id/kbtNoiseTestF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center"
        app:textSize="14sp"
        app:layout_constraintTop_toBottomOf="@+id/havNoiseTestD"
        app:layout_constraintLeft_toLeftOf="@+id/havNoiseTestF"
        app:layout_constraintRight_toRightOf="@+id/havNoiseTestF"/>
    <ltd.kcdevice.view.VerticalAdjView
        android:id="@+id/havNoiseTestF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="50dp"
        app:layout_constraintTop_toBottomOf="@+id/kbtNoiseTestF"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <!--后置右-->
    <ltd.advskin.view.KcBtnText
        android:id="@+id/kbtNoiseTestG"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center"
        app:textSize="14sp"
        app:layout_constraintTop_toBottomOf="@+id/havNoiseTestE"
        app:layout_constraintLeft_toLeftOf="@+id/havNoiseTestG"
        app:layout_constraintRight_toRightOf="@+id/havNoiseTestG"/>
    <ltd.kcdevice.view.VerticalAdjView
        android:id="@+id/havNoiseTestG"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="50dp"
        app:layout_constraintTop_toBottomOf="@+id/kbtNoiseTestG"
        app:layout_constraintRight_toRightOf="parent"/>

    <!--LIPS-->
    <ltd.advskin.view.KcBtnText
        android:id="@+id/kbtNoiseTestI"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginBottom="5dp"
        app:text="LIPS"
        app:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/havNoiseTestI"
        app:layout_constraintLeft_toLeftOf="@+id/havNoiseTestI"
        app:layout_constraintRight_toRightOf="@+id/havNoiseTestI"/>
    <ltd.kcdevice.view.HorizontalAdjView
        android:id="@+id/havNoiseTestI"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/havNoiseTestG"/>

</androidx.constraintlayout.widget.ConstraintLayout>
