<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ltd.advskin.view.KcHeaderView
        android:id="@+id/khvSpeakerSetup"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <!--中置-->
    <ltd.kcdevice.view.SpeakerSetupView
        android:id="@+id/ssvSpeakerSetupA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/khvSpeakerSetup"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

    <!--低音-->
    <ltd.kcdevice.view.SpeakerSetupView
        android:id="@+id/ssvSpeakerSetupB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@+id/khvSpeakerSetup"
        app:layout_constraintLeft_toRightOf="@+id/ssvSpeakerSetupA"
        app:layout_constraintRight_toLeftOf="@+id/ssvSpeakerSetupD"/>

    <!--前置-->
    <ltd.kcdevice.view.SpeakerSetupView
        android:id="@+id/ssvSpeakerSetupC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="90dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintTop_toBottomOf="@+id/khvSpeakerSetup"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <!--中置-->
    <ltd.kcdevice.view.SpeakerSetupView
        android:id="@+id/ssvSpeakerSetupD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="90dp"
        android:layout_marginRight="10dp"
        app:layout_constraintTop_toBottomOf="@+id/khvSpeakerSetup"
        app:layout_constraintRight_toRightOf="parent"/>

    <!--环绕-->
    <ltd.kcdevice.view.SpeakerSetupView
        android:id="@+id/ssvSpeakerSetupE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/ssvSpeakerSetupC"
        app:layout_constraintLeft_toLeftOf="@+id/ssvSpeakerSetupC"/>

    <!--环绕-->
    <ltd.kcdevice.view.SpeakerSetupView
        android:id="@+id/ssvSpeakerSetupF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/ssvSpeakerSetupD"
        app:layout_constraintRight_toRightOf="@+id/ssvSpeakerSetupD"/>

    <!--后置-->
    <ltd.kcdevice.view.SpeakerSetupView
        android:id="@+id/ssvSpeakerSetupG"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="40dp"
        app:layout_constraintTop_toBottomOf="@+id/ssvSpeakerSetupE"
        app:layout_constraintLeft_toLeftOf="@+id/ssvSpeakerSetupE"/>

    <!--后置-->
    <ltd.kcdevice.view.SpeakerSetupView
        android:id="@+id/ssvSpeakerSetupH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginRight="40dp"
        app:layout_constraintTop_toBottomOf="@+id/ssvSpeakerSetupF"
        app:layout_constraintRight_toRightOf="@+id/ssvSpeakerSetupF"/>

</androidx.constraintlayout.widget.ConstraintLayout>
