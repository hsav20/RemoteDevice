<?xml version="1.0" encoding="utf-8"?>
<ltd.advskin.view.KcLayoutCard
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="80dp"
    app:cardCornerRadius="5dp"
    app:cardElevation="5dp"
    app:cardMaxElevation="5dp">

    <HorizontalScrollView android:layout_width="match_parent"
        android:layout_height="wrap_content"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        xmlns:android="http://schemas.android.com/apk/res/android">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical">
            <ltd.advskin.view.KcBtnProgress
                android:id="@+id/kbpInput1"
                android:visibility="gone"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                app:textSize="14sp"
                app:corner="8"/>
            <ltd.advskin.view.KcBtnProgress
                android:id="@+id/kbpInput2"
                android:visibility="gone"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                app:textSize="14sp"
                app:corner="8"/>
            <ltd.advskin.view.KcBtnProgress
                android:id="@+id/kbpInput3"
                android:visibility="gone"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                app:textSize="14sp"
                app:corner="8"/>
            <ltd.advskin.view.KcBtnProgress
                android:id="@+id/kbpInput4"
                android:visibility="gone"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                app:textSize="14sp"
                app:corner="8"/>
            <ltd.advskin.view.KcBtnProgress
                android:id="@+id/kbpInput5"
                android:visibility="gone"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                app:textSize="14sp"
                app:corner="8"/>
            <ltd.advskin.view.KcBtnProgress
                android:id="@+id/kbpInput6"
                android:visibility="gone"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                app:textSize="14sp"
                app:corner="8"/>
            <ltd.advskin.view.KcBtnProgress
                android:id="@+id/kbpInput7"
                android:visibility="gone"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                app:textSize="14sp"
                app:corner="8"/>
            <ltd.advskin.view.KcBtnProgress
                android:id="@+id/kbpInput8"
                android:visibility="gone"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                app:textSize="14sp"
                app:corner="8"/>
            <ltd.advskin.view.KcBtnProgress
                android:id="@+id/kbpInput9"
                android:visibility="gone"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                app:textSize="14sp"
                app:corner="8"/>
            <ltd.advskin.view.KcBtnProgress
                android:id="@+id/kbpInput10"
                android:visibility="gone"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                app:textSize="14sp"
                app:corner="8"/>
        </LinearLayout>
    </HorizontalScrollView>

</ltd.advskin.view.KcLayoutCard>
